<h2>Sign Up</h2>


<%= render "error_messages", target: user %>

<%= form_for @user do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
